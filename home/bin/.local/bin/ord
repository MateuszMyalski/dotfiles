#!/usr/bin/env python3
import sys
import os

def main():
    name = os.path.basename(sys.argv[0])
    data = sys.stdin.read() if not sys.stdin.isatty() else " ".join(sys.argv[1:])
    if not data:
        print(f"Usage: {name} [STRING] or echo 'A' | {name}", file=sys.stderr)
        sys.exit(1)

    codes = [str(ord(ch)) for ch in data]
    print(" ".join(codes))

if __name__ == "__main__":
    main()