#!/usr/bin/env python3
import sys
import os

def main():
    name = os.path.basename(sys.argv[0])
    data = sys.stdin.read() if not sys.stdin.isatty() else " ".join(sys.argv[1:])
    if not data:
        print(f"Usage: {name} [NUMBERS] or echo '65 66' | {name}", file=sys.stderr)
        sys.exit(1)

    try:
        chars = [chr(int(x)) for x in data.split()]
        print("".join(chars), end="")
    except ValueError:
        print("Error: input must be space-separated ASCII codes", file=sys.stderr)
        sys.exit(2)

    print()

if __name__ == "__main__":
    main()
